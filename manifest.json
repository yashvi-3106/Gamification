{
    "manifest_version": 3,
    "name": "Productivity Gamification",
    "version": "1.0",
    "description": "Gamify your work and break cycles to stay productive!",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "permissions": ["storage", "activeTab", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "host_permissions": ["*://*/*"]
  }
  